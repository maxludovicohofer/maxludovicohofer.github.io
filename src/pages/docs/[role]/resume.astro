---
import DefaultResume from "@pages/docs/resume.astro";
import { getCollection } from "astro:content";
import { getPath } from "@integrations/text";

export const getStaticPaths = async () =>
  (await getCollection("roles")).slice(1).map((entry) => ({
    params: { role: getPath(entry.id) },
    props: { entry },
  }));
---

<DefaultResume />
