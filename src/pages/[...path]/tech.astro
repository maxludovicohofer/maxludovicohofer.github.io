---
import TechList from "@components/TechList.astro";
import { capitalize } from "@integrations/text";
import Page from "@layouts/Page.astro";
import { getStaticPaths as getBasePaths } from "@pages/index.astro";
import Post from "@post/Post.astro";

export const getStaticPaths = getBasePaths;
---

<Page description="My tech stack" backHref=""
  ><Post contentProps={{ tag: "div" }} fullWidth class="max-w-[170rem]"
    ><Fragment slot="title">{capitalize("tech")}</Fragment>
    <TechList
      class="gap-4 sm:grid-cols-2 lg:min-h-[calc(100vh-12rem)] lg:grid-cols-3 2xl:grid-cols-4"
    /></Post
  >
</Page>
