---
import type { ComponentProps } from "astro/types";
import DocumentMedia from "./DocumentMedia.astro";

interface Props extends ComponentProps<typeof DocumentMedia> {
  resolution?: number;
}

const { resolution = 7, ...props } = Astro.props;
---

<DocumentMedia
  class="shadow-none !h-32"
  width={resolution * 32}
  loading="eager"
  {...props}
/>
